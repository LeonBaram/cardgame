{
  "name": "cardgame",
  "version": "1.0.0",
  "description": "web app for playing cards",
  "scripts": {
    "dev": "concurrently \"npx parcel watch --dist-dir dist/client\" \"npx tsc -w\" \"npx nodemon dist/server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonBaram/cardgame.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeonBaram/cardgame/issues"
  },
  "homepage": "https://github.com/LeonBaram/cardgame#readme",
  "workspaces": [
    "client",
    "server"
  ],
  "targets": {
    "default": {
      "source": "client/index.html",
      "distDir": "./dist/client"
    }
  },
  "devDependencies": {
    "hjson": "^3.2.2",
    "json-schema-to-typescript": "^10.1.5",
    "concurrently": "^7.2.1",
    "path-browserify": "^1.0.1"
  }
