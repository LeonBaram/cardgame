{
  "name": "cardgame",
  "version": "1.0.0",
  "description": "web app for playing cards",
  "scripts": {
    "build": "esbuild memes.ts --outfile=memes.js --bundle --minify --sourcemap",
    "dev": "concurrently \"parcel watch --dist-dir dist/client\" \"tsc -w\" \"nodemon dist/server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LeonBaram/cardgame.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LeonBaram/cardgame/issues"
  },
  "homepage": "https://github.com/LeonBaram/cardgame#readme",
  "targets": {
    "default": {
      "source": "client/welcome/index.html",
      "distDir": "./dist/client"
    }
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.6.0",
    "@types/express": "^4.17.13",
    "@types/ws": "^8.5.3",
    "concurrently": "^7.2.1",
    "esbuild": "^0.14.49",
    "events": "^3.3.0",
    "hjson": "^3.2.2",
    "json-schema-to-typescript": "^10.1.5",
    "nodemon": "^2.0.19",
    "phaser": "^3.55.2",
    "typescript": "^4.7.4"
  },
  "optionalDependencies": {
    "bufferutil": "^4.0.6",
    "utf-8-validate": "^5.0.9"
  },
  "dependencies": {
    "express": "^4.18.1",
    "ws": "^8.8.1"
  }
